@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Otrem>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-opticaltrem.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-opticaltrem.css> ;
        modgui:screenshot <modgui/screenshot-rkr-opticaltrem.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-opticaltrem.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "rkr OpticalTrem" ;
        modgui:model "lata" ;
        modgui:panel "7-knobs" ;
        modgui:color "racing" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "DEPTH" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "TEMPO" ;
            lv2:name "Rate" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "TYPE" ;
            lv2:name "LFO Type" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "STDF" ;
            lv2:name "LFO L/R Delay" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "PAN" ;
            lv2:name "Panning" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "INV" ;
            lv2:name "Invert" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "RND" ;
            lv2:name "Randomness" ;
        ] ;
    ] .