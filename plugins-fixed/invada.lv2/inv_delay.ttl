	lv2:name "R Out" ;
	lv2:default -60.0 ;
	lv2:minimum -60.0 ;
	lv2:maximum 6.0 ;
	units:unit units:db ;
    ] , [
	a lv2:OutputPort, lv2:ControlPort ;
	lv2:index 17 ;
	lv2:symbol "LFOlamp" ;
	lv2:name "LFO Lamp" ;
	lv2:default 0.0 ;
	lv2:minimum 0.0 ;
	lv2:maximum 4.0 ;
    ] , [
	a lv2:InputPort, lv2:AudioPort ;
	lv2:index 18 ;
