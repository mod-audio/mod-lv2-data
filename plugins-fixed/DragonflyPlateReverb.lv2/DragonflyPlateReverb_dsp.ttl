@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .
@prefix mod:   <http://moddevices.com/ns/mod#> .
@prefix opts:  <http://lv2plug.in/ns/ext/options#> .
@prefix pg:    <http://lv2plug.in/ns/ext/port-groups#> .
@prefix patch: <http://lv2plug.in/ns/ext/patch#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsz:   <http://lv2plug.in/ns/ext/resize-port#> .
@prefix spdx:  <http://spdx.org/rdf/terms#> .
@prefix unit:  <http://lv2plug.in/ns/extensions/units#> .

<urn:dragonfly:plate>
    a lv2:ReverbPlugin, lv2:Plugin, doap:Project ;

    lv2:extensionData opts:interface ,
                      <http://lv2plug.in/ns/ext/state#interface> ,
                      <http://lv2plug.in/ns/ext/worker#interface> ;

    lv2:optionalFeature <http://lv2plug.in/ns/lv2core#hardRTCapable> ,
                        <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature opts:options ,
                        <http://lv2plug.in/ns/ext/urid#map> ,
                        <http://lv2plug.in/ns/ext/worker#schedule> ;

    opts:supportedOption <http://lv2plug.in/ns/ext/buf-size#nominalBlockLength> ,
                         <http://lv2plug.in/ns/ext/buf-size#maxBlockLength> ,
                         <http://lv2plug.in/ns/ext/parameters#sampleRate> ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] ,
    [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_audio_in_2" ;
        lv2:name "Audio Input 2" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] ,
    [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "lv2_audio_out_2" ;
        lv2:name "Audio Output 2" ;
    ] ;

    lv2:port [
        a lv2:InputPort, atom:AtomPort ;
        lv2:index 4 ;
        lv2:name "Events Input" ;
        lv2:symbol "lv2_events_in" ;
        rsz:minimumSize 2048 ;
        atom:bufferType atom:Sequence ;
        atom:supports atom:String ;
    ] ;

    lv2:port [
        a lv2:OutputPort, atom:AtomPort ;
        lv2:index 5 ;
        lv2:name "Events Output" ;
        lv2:symbol "lv2_events_out" ;
        rsz:minimumSize 2048 ;
        atom:bufferType atom:Sequence ;
        atom:supports atom:String ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "Dry Level" ;
        lv2:symbol "dry_level" ;
        lv2:default 80 ;
        lv2:minimum 0 ;
        lv2:maximum 100 ;
        unit:unit unit:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "Wet Level" ;
        lv2:symbol "early_level" ;
        lv2:default 20 ;
        lv2:minimum 0 ;
        lv2:maximum 100 ;
        unit:unit unit:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "Algorithm" ;
        lv2:symbol "algorithm" ;
        lv2:portProperty lv2:enumeration , lv2:integer ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 2 ;
        lv2:scalePoint
        [
            rdfs:label "Simple" ;
            rdf:value 0
        ] , [
            rdfs:label "Nested" ;
            rdf:value 1
        ] , [
            rdfs:label "Tank" ;
            rdf:value 2
        ]
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name "Width" ;
        lv2:symbol "width" ;
        lv2:default 100 ;
        lv2:minimum 50 ;
        lv2:maximum 150 ;
        unit:unit unit:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 10 ;
        lv2:name "Predelay" ;
        lv2:symbol "predelay" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 100 ;
        unit:unit unit:ms ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 11 ;
        lv2:name "Decay" ;
        lv2:symbol "decay" ;
        lv2:default 0.40000000596 ;
        lv2:minimum 0.10000000149 ;
        lv2:maximum 10 ;
        unit:unit unit:s ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 12 ;
        lv2:name "Low Cut" ;
        lv2:symbol "low_cut" ;
        lv2:default 200 ;
        lv2:minimum 0 ;
        lv2:maximum 200 ;
        unit:unit unit:hz ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 13 ;
        lv2:name "High Cut" ;
        lv2:symbol "high_cut" ;
        lv2:default 16000 ;
        lv2:minimum 1000 ;
        lv2:maximum 16000 ;
        unit:unit unit:hz ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 14 ;
        lv2:name "Dampen" ;
        lv2:symbol "early_damp" ;
        lv2:default 13000 ;
        lv2:minimum 1000 ;
        lv2:maximum 16000 ;
        unit:unit unit:hz ;
    ] ;

    rdfs:comment "A plate reverb based on Freeverb3 STRev" ;

    mod:brand "Dragonfly" ;
    mod:label "Plate Reverb" ;

    doap:name "Dragonfly Plate Reverb" ;
    doap:license <http://www.gnu.org/licenses/gpl-3.0.html> ;

    doap:maintainer [
        foaf:name "Michael Willis" ;
        foaf:homepage <https://michaelwillis.github.io/dragonfly-reverb> ;
    ] ;

    lv2:microVersion 6 ;
    lv2:minorVersion 4 .

