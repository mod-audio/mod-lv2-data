@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix epp:   <http://lv2plug.in/ns/ext/port-props#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix mod: <http://moddevices.com/ns/mod#> .

<http://faust-lv2.googlecode.com/Granulator>
       a lv2:Plugin ;
       doap:name "Granulator" ;
       lv2:binary <Granulator.so> ;
       lv2:optionalFeature epp:supportsStrictBounds ;
       doap:maintainer [
              foaf:name "Mayank Sanganeria";
              foaf:homepage <http://www.e7mac.com/experiments/>;
       ];
       lv2:minorVersion 1;
       lv2:microVersion 0;
       doap:license <http://usefulinc.com/doap/licenses/gpl> ;
       rdfs:comment "A realtime granulator plays small chunks of audio in the delay time setted"
       mod:brand "mayank";

    lv2:port [
	a lv2:ControlPort ;
	a lv2:InputPort ;
	lv2:index 0 ;
	lv2:symbol "delay_length" ;
	lv2:name "delay length" ;
	lv2:default 10.0 ;
	lv2:minimum 0.5 ;
	lv2:maximum 10.0 ;
    ] , [
	a lv2:ControlPort ;
	a lv2:InputPort ;
	lv2:index 1 ;
	lv2:symbol "grain_density" ;
	lv2:name "grain density" ;
	lv2:default 2.0 ;
	lv2:minimum 2.0 ;
	lv2:maximum 16.0 ;
    ] , [
	a lv2:ControlPort ;
	a lv2:InputPort ;
	lv2:index 2 ;
	lv2:symbol "grain_length" ;
	lv2:name "grain length" ;
	lv2:default 0.1 ;
	lv2:minimum 0.01 ;
	lv2:maximum 0.5 ;
    ] , [
	a lv2:ControlPort ;
	a lv2:InputPort ;
	lv2:index 3 ;
	lv2:symbol "mix" ;
	lv2:name "mix" ;
	lv2:default 0.5 ;
	lv2:minimum 0.0 ;
	lv2:maximum 1.0 ;
    ] , [
	a lv2:AudioPort ;
	a lv2:InputPort ;
	lv2:index 4 ;
	lv2:symbol "in0" ;
	lv2:name "in0" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:InputPort ;
	lv2:index 5 ;
	lv2:symbol "in1" ;
	lv2:name "in1" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:OutputPort ;
	lv2:index 6 ;
	lv2:symbol "out0" ;
	lv2:name "out0" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:OutputPort ;
	lv2:index 7 ;
	lv2:symbol "out1" ;
	lv2:name "out1" ;
    ]
.
