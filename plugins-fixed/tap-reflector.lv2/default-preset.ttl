@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://moddevices.com/plugins/tap/reflector> ;
	lv2:port [
		lv2:symbol "dry" ;
		pset:value 0.0
	] , [
		lv2:symbol "fragment" ;
		pset:value 1000.0
	] , [
		lv2:symbol "wet" ;
		pset:value 0.0
	] .

