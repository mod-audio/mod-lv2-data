@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://moddevices.com/plugins/tal-reverb-2>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-tal-reverb-ii.html> ;
        modgui:stylesheet <modgui/stylesheet-tal-reverb-ii.css> ;
        modgui:javascript<modgui/javascript-tal-reverb-ii.js> ;
        modgui:screenshot <modgui/screenshot-tal-reverb-ii.png> ;
        modgui:thumbnail <modgui/thumbnail-tal-reverb-ii.png> ;
        modgui:brand "TAL" ;
        modgui:label "Reverb-II" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "pink" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "pre_delay" ;
            lv2:name "pre delay" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "room_size" ;
            lv2:name "room size" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "low_shelf_frequency" ;
            lv2:name "low shelf frequency" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "high_shelf_frequency" ;
            lv2:name "high shelf frequency" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "peak_frequency" ;
            lv2:name "peak frequency" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "low_shelf_gain" ;
            lv2:name "low shelf gain" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "high_shelf_gain" ;
            lv2:name "high shelf gain" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "peak_gain" ;
            lv2:name "peak gain" ;
        ] , [
            lv2:index 8 ;
            lv2:symbol "stereo_width" ;
            lv2:name "stereo Width" ;
        ] , [
            lv2:index 9 ;
            lv2:symbol "stereo_input" ;
            lv2:name "stereo input" ;
        ] , [
            lv2:index 10 ;
            lv2:symbol "dry" ;
            lv2:name "dry" ;
        ] , [
            lv2:index 11 ;
            lv2:symbol "wet" ;
            lv2:name "wet" ;
        ] ;
    ] .
