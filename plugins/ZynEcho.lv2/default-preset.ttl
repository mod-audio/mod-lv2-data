@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://zynaddsubfx.sourceforge.net/fx#Echo> ;
	lv2:port [
		lv2:symbol "damp" ;
		pset:value 0
	] , [
		lv2:symbol "delay" ;
		pset:value 35
	] , [
		lv2:symbol "fb" ;
		pset:value 59
	] , [
		lv2:symbol "lrcross" ;
		pset:value 30
	] , [
		lv2:symbol "lrdelay" ;
		pset:value 64
	] .

