@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://moddevices.com/plugins/mda/Dither> ;
	lv2:port [
		lv2:symbol "dc_trim" ;
		pset:value 0.0
	] , [
		lv2:symbol "dith_amp" ;
		pset:value 2.0
	] , [
		lv2:symbol "dither" ;
		pset:value 0
	] , [
		lv2:symbol "word_len" ;
		pset:value 16
	] , [
		lv2:symbol "zoom" ;
		pset:value 0.0
	] .

