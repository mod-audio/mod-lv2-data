@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_hyperion_#_hyperion_>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxhyperion.html> ;
        modgui:stylesheet <modgui/stylesheet-gxhyperion.css> ;
        modgui:screenshot <modgui/screenshot-gxhyperion.png> ;
        modgui:thumbnail <modgui/thumbnail-gxhyperion.png> ;
        modgui:brand "Guitarix" ;
        modgui:label "GxHyperion" ;
        modgui:model "boxy" ;
        modgui:panel "2-knobs" ;
        modgui:color "red" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "INTENSITY" ;
            lv2:name "INTENSITY" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "VOLUME" ;
            lv2:name "VOLUME" ;
        ] ;
    ] .