@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .
@prefix unit: <http://lv2plug.in/ns/extensions/units#> .

<http://kxstudio.sf.net/carla/plugins/zyndistortion>
    a lv2:ModulatorPlugin, lv2:Plugin ;

    lv2:requiredFeature <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ,
                        <http://lv2plug.in/ns/ext/buf-size#fixedBlockLength> ,
                        <http://lv2plug.in/ns/ext/options#options> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    lv2:extensionData <http://lv2plug.in/ns/ext/options#interface> ;
    lv2:extensionData <http://kxstudio.sf.net/ns/lv2ext/programs#Interface> ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_freewheel" ;
        lv2:name "Freewheel" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
        lv2:designation <http://lv2plug.in/ns/lv2core#freeWheeling> ;
        lv2:portProperty lv2:toggled, <http://lv2plug.in/ns/ext/port-props#notOnGUI> ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] , [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "lv2_audio_in_2" ;
        lv2:name "Audio Input 2" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] , [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 4 ;
        lv2:symbol "lv2_audio_out_2" ;
        lv2:name "Audio Output 2" ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "l_r_cross" ;
        lv2:name "L/R Cross" ;
        lv2:default 35.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 127.000000 ;
        lv2:portProperty lv2:integer ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "drive" ;
        lv2:name "Drive" ;
        lv2:default 56.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 127.000000 ;
        lv2:portProperty lv2:integer ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "level" ;
        lv2:name "Level" ;
        lv2:default 70.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 127.000000 ;
        lv2:portProperty lv2:integer ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "type" ;
        lv2:name "Type" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 13.000000 ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [ rdfs:label "Arctangent" ;
                         rdf:value  0.000000 ] ,
                       [ rdfs:label "Asymmetric" ;
                         rdf:value  1.000000 ] ,
                       [ rdfs:label "Pow" ;
                         rdf:value  2.000000 ] ,
                       [ rdfs:label "Sine" ;
                         rdf:value  3.000000 ] ,
                       [ rdfs:label "Quantisize" ;
                         rdf:value  4.000000 ] ,
                       [ rdfs:label "Zigzag" ;
                         rdf:value  5.000000 ] ,
                       [ rdfs:label "Limiter" ;
                         rdf:value  6.000000 ] ,
                       [ rdfs:label "Upper Limiter" ;
                         rdf:value  7.000000 ] ,
                       [ rdfs:label "Lower Limiter" ;
                         rdf:value  8.000000 ] ,
                       [ rdfs:label "Inverse Limiter" ;
                         rdf:value  9.000000 ] ,
                       [ rdfs:label "Clip" ;
                         rdf:value  10.000000 ] ,
                       [ rdfs:label "Asym2" ;
                         rdf:value  11.000000 ] ,
                       [ rdfs:label "Pow2" ;
                         rdf:value  12.000000 ] ,
                       [ rdfs:label "Sigmoid" ;
                         rdf:value  13.000000 ] ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "negate" ;
        lv2:name "Negate" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
        lv2:portProperty lv2:toggled ;
        lv2:portProperty lv2:integer ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "low_pass_filter" ;
        lv2:name "Low-Pass Filter" ;
        lv2:default 96.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 127.000000 ;
        lv2:portProperty lv2:integer ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 11 ;
        lv2:symbol "high_pass_filter" ;
        lv2:name "High-Pass Filter" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 127.000000 ;
        lv2:portProperty lv2:integer ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 12 ;
        lv2:symbol "stereo" ;
        lv2:name "Stereo" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
        lv2:portProperty lv2:toggled ;
        lv2:portProperty lv2:integer ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 13 ;
        lv2:symbol "pre_filtering" ;
        lv2:name "Pre-Filtering" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
        lv2:portProperty lv2:toggled ;
        lv2:portProperty lv2:integer ;
    ] ;

    doap:name "ZynDistortion" ;
    doap:maintainer [ foaf:name "falkTX, Mark McCurry, Nasca Octavian Paul" ] .
