@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix mod:  <http://moddevices.com/ns/mod#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsz:  <http://lv2plug.in/ns/ext/resize-port#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://distrho.sf.net/plugins/Modulay>
    a lv2:DelayPlugin, lv2:Plugin ;

    lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ;

    lv2:optionalFeature <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature <http://lv2plug.in/ns/ext/options#options> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "morph" ;
        lv2:symbol "morph" ;
        lv2:default 50.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        units:unit units:pc
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name "repeats" ;
        lv2:symbol "repeats" ;
        lv2:default 75.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        units:unit units:pc
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "rate" ;
        lv2:symbol "rate" ;
        lv2:default 2.000000 ;
        lv2:minimum 0.100000 ;
        lv2:maximum 10.000000 ;
        units:unit units:hz
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 2 ;
        lv2:name "time" ;
        lv2:symbol "time" ;
        lv2:default 500.000000 ;
        lv2:minimum 20.000000 ;
        lv2:maximum 1000.000000 ;
        units:unit units:ms
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "mix" ;
        lv2:symbol "mix" ;
        lv2:default 100.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        units:unit units:pc
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "tone" ;
        lv2:symbol "tone" ;
        lv2:default 2500.000000 ;
        lv2:minimum 500.000000 ;
        lv2:maximum 6000.000000 ;
        units:unit units:hz
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "depth" ;
        lv2:symbol "depth" ;
        lv2:default 1.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 5.000000 ;
        units:unit units:ms
    ] ;

    rdfs:comment """
Modulay is a delay with variable types of modulation based on the settings that are set.
""" ;

    mod:brand "SHIRO" ;
    mod:label "Modulay" ;

    doap:name "Modulay" ;
    doap:license "ISC" ;

    doap:maintainer [
        foaf:name "DISTRHO" ;
        foaf:homepage <https://github.com/DISTRHO/DPF-Max-Gen> ;
    ] ;

    lv2:microVersion 1 ;
    lv2:minorVersion 0 .

