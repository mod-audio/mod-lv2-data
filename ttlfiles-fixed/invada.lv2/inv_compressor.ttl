<http://invadarecords.com/plugins/lv2/compressor/mono>
    a lv2:Plugin, lv2:CompressorPlugin ;
    doap:name "Invada Compressor (mono)" ;
    mod:brand "Invada" ;
    mod:label "Compressor M" ;
    lv2:project <http://invadarecords.com/plugins/lv2/> ;
    lv2:minorVersion 2;
    lv2:microVersion 1;
