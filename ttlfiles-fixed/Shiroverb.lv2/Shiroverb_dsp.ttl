@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix mod:  <http://moddevices.com/ns/mod#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rsz:  <http://lv2plug.in/ns/ext/resize-port#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

<http://distrho.sf.net/plugins/Shiroverb>
    a lv2:ReverbPlugin, lv2:Plugin ;

    lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ;

    lv2:optionalFeature <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature <http://lv2plug.in/ns/ext/options#options> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "lv2_audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "lv2_audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] ,
    [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "lv2_audio_out_2" ;
        lv2:name "Audio Output 2" ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name "roomsize" ;
        lv2:symbol "roomsize" ;
        lv2:default 150.000000 ;
        lv2:minimum 1.000000 ;
        lv2:maximum 300.000000 ;
        units:unit units:m ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "shimmer" ;
        lv2:symbol "shimmer" ;
        lv2:default 50.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        units:unit units:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "decay" ;
        lv2:symbol "decay" ;
        lv2:default 50.000000 ;
        lv2:minimum 1.000000 ;
        lv2:maximum 100.000000 ;
        units:unit units:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "damping" ;
        lv2:symbol "damping" ;
        lv2:default 50.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        units:unit units:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "ratio" ;
        lv2:symbol "ratio" ;
        lv2:default 2.000000 ;
        lv2:minimum 0.500000 ;
        lv2:maximum 2.000000 ;
        lv2:scalePoint [ rdfs:label "- Octave" ; rdf:value 0.500000 ] ;
        lv2:scalePoint [ rdfs:label "+ Fifth" ; rdf:value 1.500000 ] ;
        lv2:scalePoint [ rdfs:label "+ Octave" ; rdf:value 2.000000 ] ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "bandwidth" ;
        lv2:symbol "bandwidth" ;
        lv2:default 75.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        units:unit units:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name "mix" ;
        lv2:symbol "mix" ;
        lv2:default 50.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 100.000000 ;
        units:unit units:pc ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 10 ;
        lv2:name "tone" ;
        lv2:symbol "tone" ;
        lv2:portProperty epp:logarithmic ;
        lv2:default 1500.000000 ;
        lv2:minimum 500.000000 ;
        lv2:maximum 6000.000000 ;
        units:unit units:hz ;
    ] ;

    rdfs:comment """
Shiroverb is a shimmer-reverb based on the "Gigaverb"-genpatch, ported from the implementation by Juhana Sadeharju, and the "Pitch-Shift"-genpatch, both in Max MSP.
""" ;

    mod:brand "SHIRO" ;
    mod:label "Shiroverb" ;

    doap:name "Shiroverb" ;
    doap:license "ISC" ;

    doap:maintainer [
        foaf:name "DISTRHO" ;
        foaf:homepage <https://github.com/DISTRHO/DPF-Max-Gen> ;
    ] ;

    lv2:microVersion 1 ;
    lv2:minorVersion 0 .

