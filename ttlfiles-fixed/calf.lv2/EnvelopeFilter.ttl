@prefix : <http://calf.sourceforge.net/plugins/EnvelopeFilter#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix uiext: <http://lv2plug.in/ns/extensions/ui#> .
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix lv2midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix lv2ctx: <http://lv2plug.in/ns/dev/contexts#> .
@prefix strport: <http://lv2plug.in/ns/dev/string-port#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix ue: <http://lv2plug.in/ns/extensions/units#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix param: <http://lv2plug.in/ns/ext/parameters#> .
@prefix mod:   <http://moddevices.com/ns/mod#> .

:in a pg:StereoGroup , pg:InputGroup ;
    lv2:symbol "in" ;
    rdfs:label "Input" .

:out a pg:StereoGroup , pg:OutputGroup ;
    lv2:symbol "out" ;
    rdfs:label "Output" .

<http://calf.sourceforge.net/plugins/EnvelopeFilter> a lv2:Plugin ;
    a lv2:FilterPlugin ;
    doap:name "Calf Envelope Filter" ;
    doap:maintainer [
    foaf:name "Calf Studio Gear / Markus Schmidt" ;
    ] ;
    doap:license <http://usefulinc.com/doap/licenses/lgpl> ;
    dct:replaces <urn:ladspa:33842> ;
    lv2:optionalFeature epp:supportsStrictBounds ;
    lv2:optionalFeature lv2:hardRTCapable ;

    mod:brand "Calf";
    mod:label "Envelope filter";
    lv2:minorVersion 60;
    lv2:microVersion 0;
    rdfs:comment """
      The louder you play, the higher the filter
    """;

    pg:mainInput :in ;
    pg:mainOutput :out ;
    lv2:port [
        a lv2:InputPort ;
        a lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "in_l" ;
        lv2:name "In L" ;
        lv2:designation pg:left ;
        pg:group :in ;
    ] , [
        a lv2:InputPort ;
        a lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "in_r" ;
        lv2:name "In R" ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:designation pg:right ;
        pg:group :in ;
    ] , [
        a lv2:InputPort ;
        a lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "Audiosidechain" ;
        lv2:name "Sidechain" ;
        lv2:portProperty lv2:connectionOptional ;
    ] , [
        a lv2:InputPort ;
        a lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "sidechain2" ;
        lv2:name "Sidechain 2" ;
        lv2:portProperty lv2:connectionOptional ;
    ] , [
        a lv2:OutputPort ;
        a lv2:AudioPort ;
        lv2:index 4 ;
        lv2:symbol "out_l" ;
        lv2:name "Out L" ;
        lv2:designation pg:left ;
        pg:group :out ;
    ] , [
        a lv2:OutputPort ;
        a lv2:AudioPort ;
        lv2:index 5 ;
        lv2:symbol "out_r" ;
        lv2:name "Out R" ;
        lv2:designation pg:right ;
        pg:group :out ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "bypass" ;
        lv2:name "Bypass" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:toggled ;
        lv2:default 0.00000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "level_in" ;
        lv2:name "Input Gain" ;
        lv2:default 1.00000 ;
        lv2:minimum 0.0156250 ;
        lv2:maximum 64.0000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "level_out" ;
        lv2:name "Output Gain" ;
        lv2:default 2.00000 ;
        lv2:minimum 0.0156250 ;
        lv2:maximum 64.0000 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "meter_inL" ;
        lv2:name "Meter-InL" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "meter_inR" ;
        lv2:name "Meter-InR" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 11 ;
        lv2:symbol "meter_outL" ;
        lv2:name "Meter-OutL" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 12 ;
        lv2:symbol "meter_outR" ;
        lv2:name "Meter-OutR" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 13 ;
        lv2:symbol "clip_inL" ;
        lv2:name "0dB-InL" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 14 ;
        lv2:symbol "clip_inR" ;
        lv2:name "0dB-InR" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 15 ;
        lv2:symbol "clip_outL" ;
        lv2:name "0dB-OutL" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 16 ;
        lv2:symbol "clip_outR" ;
        lv2:name "0dB-OutR" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 17 ;
        lv2:symbol "mix" ;
        lv2:name "Mix" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.850000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 18 ;
        lv2:symbol "res" ;
        lv2:name "Resonance" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 2.00000 ;
        lv2:minimum 0.707000 ;
        lv2:maximum 32.0000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 19 ;
        lv2:symbol "mode" ;
        lv2:name "Mode" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [ rdfs:label "12dB/oct Lowpass"; rdf:value 0 ] ;
        lv2:scalePoint [ rdfs:label "24dB/oct Lowpass"; rdf:value 1 ] ;
        lv2:scalePoint [ rdfs:label "36dB/oct Lowpass"; rdf:value 2 ] ;
        lv2:scalePoint [ rdfs:label "12dB/oct Highpass"; rdf:value 3 ] ;
        lv2:scalePoint [ rdfs:label "24dB/oct Highpass"; rdf:value 4 ] ;
        lv2:scalePoint [ rdfs:label "36dB/oct Highpass"; rdf:value 5 ] ;
        lv2:scalePoint [ rdfs:label "6dB/oct Bandpass"; rdf:value 6 ] ;
        lv2:scalePoint [ rdfs:label "12dB/oct Bandpass"; rdf:value 7 ] ;
        lv2:scalePoint [ rdfs:label "18dB/oct Bandpass"; rdf:value 8 ] ;
        lv2:scalePoint [ rdfs:label "6dB/oct Bandreject"; rdf:value 9 ] ;
        lv2:scalePoint [ rdfs:label "12dB/oct Bandreject"; rdf:value 10 ] ;
        lv2:scalePoint [ rdfs:label "18dB/oct Bandreject"; rdf:value 11 ] ;
        lv2:default 7  ;
        lv2:minimum 0  ;
        lv2:maximum 11 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 20 ;
        lv2:symbol "attack" ;
        lv2:name "Attack" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty epp:logarithmic ;
        lv2:default 20.0000 ;
        lv2:minimum 1.00000 ;
        lv2:maximum 500.000 ;
        ue:unit ue:ms ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 21 ;
        lv2:symbol "release" ;
        lv2:name "Release" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty epp:logarithmic ;
        lv2:default 200.000 ;
        lv2:minimum 10.0000 ;
        lv2:maximum 5000.00 ;
        ue:unit ue:ms ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 22 ;
        lv2:symbol "upper" ;
        lv2:name "Upper" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty epp:logarithmic ;
        lv2:default 3000.00 ;
        lv2:minimum 10.0000 ;
        lv2:maximum 20000.0 ;
        ue:unit ue:hz ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 23 ;
        lv2:symbol "lower" ;
        lv2:name "Lower" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty epp:logarithmic ;
        lv2:default 80.0000 ;
        lv2:minimum 10.0000 ;
        lv2:maximum 20000.0 ;
        ue:unit ue:hz ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 24 ;
        lv2:symbol "gain" ;
        lv2:name "Activation" ;
        lv2:default 1.00000 ;
        lv2:minimum 0.0156250 ;
        lv2:maximum 64.0000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 25 ;
        lv2:symbol "sidechain" ;
        lv2:name "Sidechain" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:toggled ;
        lv2:default 0.00000 ;
        lv2:minimum 0.00000 ;
        lv2:maximum 1.00000 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 26 ;
        lv2:symbol "response" ;
        lv2:name "Response" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.00000 ;
        lv2:minimum -1.00000 ;
        lv2:maximum 1.00000 ;
    ]
.
