@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix epp:   <http://lv2plug.in/ns/ext/port-props#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix mod: <http://moddevices.com/ns/mod#> .

<http://faust-lv2.googlecode.com/Prefreak>
a doap:Project ;
  doap:name "Prefreak" ;
  doap:homepage <https://github.com/pjotrompet>;
  doap:maintainer [
    foaf:name "Pjotr Lasschuit";
    foaf:homepage <https://github.com/pjotrompet>;
  ];
  doap:developer [
      a foaf:Person ;
      foaf:name "Pjotr Lasschuit" ;
      foaf:mbox <mailto:ploplolma@gmail.com>;
  ] .


<http://faust-lv2.googlecode.com/Prefreak>
       a lv2:Plugin ,
         lv2:ReverbPlugin ;
       doap:name "Prefreak" ;
       lv2:binary <Prefreak.so> ;
       lv2:optionalFeature epp:supportsStrictBounds ;
       mod:brand "Freaked" ;
       lv2:minorVersion 0;
       lv2:microVersion 1;
       doap:license <http://usefulinc.com/doap/licenses/gpl> ;
       rdfs:comment "A blurry pre-delay; can be used in combination with Prefreak to complete the reverb";

    lv2:port [
	a lv2:ControlPort ;
	a lv2:InputPort ;
	lv2:index 0 ;
	lv2:symbol "Blur" ;
	lv2:name "Blur" ;
	lv2:default 0.6 ;
	lv2:minimum 0.0 ;
	lv2:maximum 15.0 ;
    ] , [
	a lv2:ControlPort ;
	a lv2:InputPort ;
	lv2:index 1 ;
	lv2:symbol "Level" ;
	lv2:name "Level" ;
	lv2:default 0.2 ;
	lv2:minimum 0.0 ;
	lv2:maximum 1.5 ;
    ] , [
	a lv2:AudioPort ;
	a lv2:InputPort ;
	lv2:index 2 ;
	lv2:symbol "in0" ;
	lv2:name "in0" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:InputPort ;
	lv2:index 3 ;
	lv2:symbol "in1" ;
	lv2:name "in1" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:OutputPort ;
	lv2:index 4 ;
	lv2:symbol "out0" ;
	lv2:name "out0" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:OutputPort ;
	lv2:index 5 ;
	lv2:symbol "out1" ;
	lv2:name "out1" ;
    ]
.
