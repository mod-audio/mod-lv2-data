@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix epp:   <http://lv2plug.in/ns/ext/port-props#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix mod: <http://moddevices.com/ns/mod#> .

<http://faust-lv2.googlecode.com/Freaktail>
a doap:Project ;
  doap:name "Freaktail" ;
  doap:homepage <https://github.com/pjotrompet>;
  doap:maintainer [
    foaf:name "Pjotr Lasschuit";
    foaf:homepage <https://github.com/pjotrompet>;
  ];
  doap:developer [
      a foaf:Person ;
      foaf:name "Pjotr Lasschuit" ;
      foaf:mbox <mailto:ploplolma@gmail.com>;
  ] .
<http://faust-lv2.googlecode.com/Freaktail>
       a lv2:Plugin ;
       doap:name "Freaktail" ;
       lv2:binary <Freaktail.so> ;
       lv2:optionalFeature epp:supportsStrictBounds ;
       mod:brand "Freaked" ;
       lv2:minorVersion 1;
       lv2:microVersion 0;
       doap:license <http://usefulinc.com/doap/licenses/gpl> ;
       rdfs:comment "The tail of a Reverb, can be used in combination with Prefreak to complete the reverb";
    lv2:port [
	a lv2:ControlPort ;
	a lv2:InputPort ;
	lv2:index 0 ;
	lv2:symbol "Decay" ;
	lv2:name "Decay" ;
	lv2:default 0.81 ;
	lv2:minimum 0.1 ;
	lv2:maximum 1.0 ;
    ] , [
	a lv2:ControlPort ;
	a lv2:InputPort ;
	lv2:index 1 ;
	lv2:symbol "Roomsize" ;
	lv2:name "Roomsize" ;
	lv2:default 0.8 ;
	lv2:minimum 0.01 ;
	lv2:maximum 5.0 ;
    ] , [
	a lv2:ControlPort ;
	a lv2:InputPort ;
	lv2:index 2 ;
	lv2:symbol "Tail_level" ;
	lv2:name "Tail level" ;
	lv2:default 0.81 ;
	lv2:minimum 0.1 ;
	lv2:maximum 1.0 ;
    ] , [
	a lv2:AudioPort ;
	a lv2:InputPort ;
	lv2:index 3 ;
	lv2:symbol "in0" ;
	lv2:name "in0" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:InputPort ;
	lv2:index 4 ;
	lv2:symbol "in1" ;
	lv2:name "in1" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:OutputPort ;
	lv2:index 5 ;
	lv2:symbol "out0" ;
	lv2:name "out0" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:OutputPort ;
	lv2:index 6 ;
	lv2:symbol "out1" ;
	lv2:name "out1" ;
    ]
.
