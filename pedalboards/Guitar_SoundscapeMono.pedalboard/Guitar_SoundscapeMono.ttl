@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix ingen: <http://drobilla.net/ns/ingen#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .
@prefix pedal: <http://moddevices.com/ns/modpedal#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

_:b1
    ingen:tail <PlateX2/outl> ;
    ingen:head <playback_1> .

_:b2
    ingen:tail <capture_1> ;
    ingen:head <triple_chorus/in> .

_:b3
    ingen:tail <triple_chorus/out3> ;
    ingen:head <reflector/input> .

_:b4
    ingen:tail <reflector/output> ;
    ingen:head <Scape/in> .

_:b5
    ingen:tail <triple_chorus/out1> ;
    ingen:head <PlateX2/inl> .

_:b6
    ingen:tail <Scape/outr> ;
    ingen:head <PlateX2/inr> .

_:b7
    ingen:tail <triple_chorus/out2> ;
    ingen:head <PlateX2/inr> .

_:b8
    ingen:tail <Scape/outl> ;
    ingen:head <PlateX2/inl> .

_:b9
    ingen:tail <PlateX2/outr> ;
    ingen:head <playback_1> .

<triple_chorus>
    ingen:canvasX 388.4 ;
    ingen:canvasY 308.0 ;
    ingen:enabled false ;
    ingen:polyphonic false ;
    lv2:microVersion 0 ;
    lv2:minorVersion 0 ;
    lv2:port <triple_chorus/in> ,
             <triple_chorus/out1> ,
             <triple_chorus/out2> ,
             <triple_chorus/out3> ,
             <triple_chorus/delay> ,
             <triple_chorus/mod_freq_1> ,
             <triple_chorus/mod_amp_1> ,
             <triple_chorus/mod_freq_2> ,
             <triple_chorus/mod_amp_2> ;
    lv2:prototype <http://drobilla.net/plugins/fomp/triple_chorus> ;
    a ingen:Block .

<triple_chorus/in>
    a lv2:AudioPort ,
        lv2:InputPort .

<triple_chorus/in>
    a lv2:AudioPort ,
        lv2:OutputPort .

<triple_chorus/mod_freq_1>
    ingen:value 0.100000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<triple_chorus/mod_freq_2>
    ingen:value 0.550000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<triple_chorus/mod_amp_1>
    ingen:value 5.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<triple_chorus/delay>
    ingen:value 22.500000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<triple_chorus/mod_amp_2>
    ingen:value 1.500000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Scape>
    ingen:canvasX 1409.8 ;
    ingen:canvasY 596.0 ;
    ingen:enabled true ;
    ingen:polyphonic false ;
    lv2:microVersion 24 ;
    lv2:minorVersion 24 ;
    lv2:port <Scape/in> ,
             <Scape/outl> ,
             <Scape/outr> ,
             <Scape/bpm> ,
             <Scape/divider> ,
             <Scape/feedback> ,
             <Scape/dry> ,
             <Scape/blend> ,
             <Scape/tune> ;
    lv2:prototype <http://moddevices.com/plugins/caps/Scape> ;
    a ingen:Block .

<Scape/in>
    a lv2:AudioPort ,
        lv2:InputPort .

<Scape/in>
    a lv2:AudioPort ,
        lv2:OutputPort .

<Scape/blend>
    ingen:value 1.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Scape/divider>
    ingen:value 3.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Scape/feedback>
    ingen:value 0.750000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Scape/dry>
    ingen:value 0.750000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Scape/tune>
    ingen:value 440.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Scape/bpm>
    ingen:value 100.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<PlateX2>
    ingen:canvasX 1917.3 ;
    ingen:canvasY 306.0 ;
    ingen:enabled true ;
    ingen:polyphonic false ;
    lv2:microVersion 24 ;
    lv2:minorVersion 24 ;
    lv2:port <PlateX2/inl> ,
             <PlateX2/inr> ,
             <PlateX2/outl> ,
             <PlateX2/outr> ,
             <PlateX2/bandwidth> ,
             <PlateX2/tail> ,
             <PlateX2/damping> ,
             <PlateX2/blend> ;
    lv2:prototype <http://moddevices.com/plugins/caps/PlateX2> ;
    a ingen:Block .

<PlateX2/inl>
    a lv2:AudioPort ,
        lv2:InputPort .

<PlateX2/inr>
    a lv2:AudioPort ,
        lv2:InputPort .

<PlateX2/inl>
    a lv2:AudioPort ,
        lv2:OutputPort .

<PlateX2/inr>
    a lv2:AudioPort ,
        lv2:OutputPort .

<PlateX2/tail>
    ingen:value 0.625000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<PlateX2/bandwidth>
    ingen:value 0.999500 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<PlateX2/damping>
    ingen:value 0.000500 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<PlateX2/blend>
    ingen:value 0.375000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<reflector>
    ingen:canvasX 863.3 ;
    ingen:canvasY 592.0 ;
    ingen:enabled true ;
    ingen:polyphonic false ;
    lv2:microVersion 2 ;
    lv2:minorVersion 2 ;
    lv2:port <reflector/input> ,
             <reflector/output> ,
             <reflector/fragment> ,
             <reflector/dry> ,
             <reflector/wet> ;
    lv2:prototype <http://moddevices.com/plugins/tap/reflector> ;
    a ingen:Block .

<reflector/input>
    a lv2:AudioPort ,
        lv2:InputPort .

<reflector/input>
    a lv2:AudioPort ,
        lv2:OutputPort .

<reflector/fragment>
    ingen:value 1000.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<reflector/wet>
    ingen:value 0.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<reflector/dry>
    ingen:value 0.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<control_in>
    atom:bufferType atom:Sequence ;
    lv2:index 0 ;
    lv2:name "Control In" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "control_in" ;
    <http://lv2plug.in/ns/ext/resize-port#minimumSize> 4096 ;
    a atom:AtomPort ,
        lv2:InputPort .

<control_out>
    atom:bufferType atom:Sequence ;
    lv2:index 1 ;
    lv2:name "Control Out" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "control_out" ;
    <http://lv2plug.in/ns/ext/resize-port#minimumSize> 4096 ;
    a atom:AtomPort ,
        lv2:OutputPort .

<capture_1>
    lv2:index 2 ;
    lv2:name "Capture 1" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "capture_1" ;
    a lv2:AudioPort ,
        lv2:InputPort .

<capture_2>
    lv2:index 3 ;
    lv2:name "Capture 2" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "capture_2" ;
    a lv2:AudioPort ,
        lv2:InputPort .

<playback_1>
    lv2:index 4 ;
    lv2:name "Playback 1" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "playback_1" ;
    a lv2:AudioPort ,
        lv2:OutputPort .

<playback_2>
    lv2:index 5 ;
    lv2:name "Playback 2" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "playback_2" ;
    a lv2:AudioPort ,
        lv2:OutputPort .

<serial_midi_in>
    atom:bufferType atom:Sequence ;
    atom:supports midi:MidiEvent ;
    lv2:index 6 ;
    lv2:name "Serial MIDI In" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "serial_midi_in" ;
    <http://lv2plug.in/ns/ext/resize-port#minimumSize> 4096 ;
    a atom:AtomPort ,
        lv2:InputPort .

<serial_midi_out>
    atom:bufferType atom:Sequence ;
    atom:supports midi:MidiEvent ;
    lv2:index 7 ;
    lv2:name "Serial MIDI In" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "serial_midi_out" ;
    <http://lv2plug.in/ns/ext/resize-port#minimumSize> 4096 ;
    a atom:AtomPort ,
        lv2:OutputPort .

<>
    doap:name "Guitar-SoundscapeMono" ;
    pedal:width 2880 ;
    pedal:height 1444 ;
    pedal:addressings <addressings.json> ;
    pedal:screenshot <screenshot.png> ;
    pedal:thumbnail <thumbnail.png> ;
    ingen:polyphony 1 ;
    ingen:arc _:b1 ,
              _:b2 ,
              _:b3 ,
              _:b4 ,
              _:b5 ,
              _:b6 ,
              _:b7 ,
              _:b8 ,
              _:b9 ;
    ingen:block <triple_chorus> ,
                <Scape> ,
                <PlateX2> ,
                <reflector> ;
    lv2:port <control_in> ,
             <control_out> ,
             <serial_midi_in> ,
             <serial_midi_out> ,
             <capture_1> ,
             <capture_2> ,
             <playback_1> ,
             <playback_2> ;
    lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ;
    a lv2:Plugin ,
        ingen:Graph ,
        pedal:Pedalboard .
