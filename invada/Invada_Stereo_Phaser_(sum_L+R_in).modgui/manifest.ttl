@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://invadarecords.com/plugins/lv2/phaser/sum>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-invada-stereo-phaser-sum-lr-in.html> ;
        modgui:stylesheet <modgui/stylesheet-invada-stereo-phaser-sum-lr-in.css> ;
        modgui:screenshot <modgui/screenshot-invada-stereo-phaser-sum-lr-in.png> ;
        modgui:thumbnail <modgui/thumbnail-invada-stereo-phaser-sum-lr-in.png> ;
        modgui:brand "Invada" ;
        modgui:label "Stereo Phaser" ;
        modgui:model "japanese" ;
        modgui:panel "5-knobs" ;
        modgui:color "green" ;
        modgui:knob "green" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "depth" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "cycle" ;
            lv2:name "Period" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "phase" ;
            lv2:name "Offset" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "width" ;
            lv2:name "Width" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "noClip" ;
            lv2:name "Soft Clip" ;
        ] ;
    ] .