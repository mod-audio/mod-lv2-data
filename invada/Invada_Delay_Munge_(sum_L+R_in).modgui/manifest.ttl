@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://invadarecords.com/plugins/lv2/delay/sum>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-invada-delay-munge-sum-lr-in.html> ;
        modgui:stylesheet <modgui/stylesheet-invada-delay-munge-sum-lr-in.css> ;
        modgui:screenshot <modgui/screenshot-invada-delay-munge-sum-lr-in.png> ;
        modgui:thumbnail <modgui/thumbnail-invada-delay-munge-sum-lr-in.png> ;
        modgui:brand "Invada" ;
        modgui:label "Delay Munge" ;
        modgui:model "japanese" ;
        modgui:panel "8-knobs" ;
        modgui:color "red" ;
        modgui:knob "yellow" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "delay1" ;
            lv2:name "Time 1" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "fb1" ;
            lv2:name "Feedb 1" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "munge" ;
            lv2:name "Munge" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "mungemode" ;
            lv2:name "Mode" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "delay2" ;
            lv2:name "Time 2" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "fb2" ;
            lv2:name "Feedb 2" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "LFOcycle" ;
            lv2:name "LFO" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "LFOdepth" ;
            lv2:name "Depth" ;
        ] ;
    ] .