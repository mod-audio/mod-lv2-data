@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://invadarecords.com/plugins/lv2/erreverb/sum>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-invada-early-reflection-reverb-sum-lr-in.html> ;
        modgui:stylesheet <modgui/stylesheet-invada-early-reflection-reverb-sum-lr-in.css> ;
        modgui:screenshot <modgui/screenshot-invada-early-reflection-reverb-sum-lr-in.png> ;
        modgui:thumbnail <modgui/thumbnail-invada-early-reflection-reverb-sum-lr-in.png> ;
        modgui:brand "Invada" ;
        modgui:label "Early Reflection" ;
        modgui:model "japanese" ;
        modgui:panel "8-knobs" ;
        modgui:color "gray" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "diffusion" ;
            lv2:name "Diffusion" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "warmth" ;
            lv2:name "Warmth" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "hpf" ;
            lv2:name "HPF" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "listFB" ;
            lv2:name "Listener (F/B)" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "roomLength" ;
            lv2:name "Length" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "roomWidth" ;
            lv2:name "Width" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "roomHeight" ;
            lv2:name "Height" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "sourceFB" ;
            lv2:name "Source (F/B)" ;
        ] ;
    ] .