@prefix blop: <http://drobilla.net/plugins/blop/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix mod: <http://moddevices.com/ns/mod#> .
@prefix morph: <http://lv2plug.in/ns/ext/morph#> .
@prefix opts: <http://lv2plug.in/ns/ext/options#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

blop:ratio
	a lv2:Plugin ,
		lv2:UtilityPlugin ;
	lv2:project blop: ;
	lv2:symbol "ratio" ;
	lv2:microVersion 0 ;
	lv2:minorVersion 0 ;
	lv2:extensionData opts:interface ;
	lv2:optionalFeature lv2:hardRTCapable ,
		urid:map ;
	lv2:port [
		a lv2:ControlPort ,
			lv2:InputPort ,
			morph:MorphPort ;
		lv2:index 0 ;
		lv2:name "Numerator" ;
		lv2:symbol "numerator" ;
		morph:supportsType lv2:CVPort
	] , [
		a lv2:ControlPort ,
			lv2:InputPort ,
			morph:MorphPort ;
		lv2:index 1 ;
		lv2:name "Denominator" ;
		lv2:symbol "denominator" ;
		morph:supportsType lv2:CVPort
	] , [
		a lv2:ControlPort ,
			lv2:OutputPort ,
			morph:AutoMorphPort ;
		lv2:index 2 ;
		lv2:name "Ratio" ;
		lv2:symbol "ratio" ;
		morph:supportsType lv2:CVPort
	] ;
	dct:replaces <urn:ladspa:2034> ,
		<urn:ladspa:2035> ,
		<urn:ladspa:2036> ,
		<urn:ladspa:2037> ;
	doap:name "Ratio" ;
	mod:brand "BLOP" ;
	mod:label "Ratio" ;
	lv2:documentation """
<p>Get the ratio between two signals.</p>
<p>To avoid divisions by zero, 0 is treated as a really small number.</p>
""" .
