@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://moddevices.com/ns/modgui#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

<http://moddevices.com/plugins/mod-devel/SwitchBox2>
a lv2:Plugin, lv2:UtilityPlugin;

doap:name "SwitchBox2";

doap:developer [
    foaf:name "Andre";
    foaf:homepage <>;
    foaf:mbox <mailto:andre_coutinho@rocketmail.com>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:devel@moddevices.com>;
    ];

    lv2:minorVersion 1;
    lv2:microVersion 1;

doap:license <GPL>;

rdfs:comment """
This switch box receives an audio input and channel it by one of it's two outputs.

""";

lv2:port
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "In";
    lv2:name "In";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "Out1";
    lv2:name "Out 1";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 2;
    lv2:symbol "Out2";
    lv2:name "Out 2";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "Switch";
    lv2:name "Channel";
    lv2:portProperty lv2:integer;
    lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [rdfs:label "1"; rdf:value 0];
        lv2:scalePoint [rdfs:label "2"; rdf:value 1];    
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
];
mod:gui [
    mod:resourcesDirectory <modgui>;
    mod:stylesheet <modgui/switchbox2.css>;
    mod:iconTemplate <modgui/icon-switchbox.html>;
    mod:templateData <modgui/data-switchbox.json>;
    mod:screenshot <modgui/SwitchBox2.png>;
    mod:thumbnail <modgui/SwitchBox2-thumb.png>;
].