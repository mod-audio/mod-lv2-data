@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_vibe_#_vibe_mono>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxvibemono.html> ;
        modgui:stylesheet <modgui/stylesheet-gxvibemono.css> ;
        modgui:screenshot <modgui/screenshot-gxvibemono.png> ;
        modgui:thumbnail <modgui/thumbnail-gxvibemono.png> ;
        modgui:brand "Guitarix" ;
        modgui:label "GxvibeMono" ;
        modgui:model "boxy" ;
        modgui:panel "5-knobs" ;
        modgui:color "red" ;
        modgui:knob "blue" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "TEMPO" ;
            lv2:name "RATE" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "DEPTH" ;
            lv2:name "DEPTH" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "FB" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WIDTH" ;
            lv2:name "WIDTH" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "WETDRY" ;
            lv2:name "MIX" ;
        ] ;
    ] .